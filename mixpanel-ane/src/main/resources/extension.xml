<extension xmlns="http://ns.adobe.com/air/extension/15.0">

    <id>com.funkypandagame.mixpanel</id>

    <versionNumber>1.0.0</versionNumber>

    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libmixpanel-ios.a</nativeLibrary>
                <initializer>MixpanelLibInitializer</initializer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libmixpanel-ios.a</nativeLibrary>
                <initializer>MixpanelLibInitializer</initializer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libInAppPurchase-Android.jar</nativeLibrary>
                <initializer>com.funkypanda.mixpanelane.Extension</initializer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>